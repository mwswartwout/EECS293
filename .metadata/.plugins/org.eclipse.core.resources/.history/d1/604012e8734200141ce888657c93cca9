package hw2;

public class SocialNetworkStatus 
{
	private enum Status
	{
		SUCCESS,
		ALREADY_VALID,
		INVALID_USERS,
		INVALID_DATE,
		ALREADY_ACTIVE,
		ALREADY_INACTIVE;
	}
	
	private Status status;
	
	public SocialNetworkStatus(Status status)
	{
		this.status = status;
	}
	
	public void setStatus(Status status)
	{
		this.status = status;
	}
	
	public Status getStatus()
	{
		return status;
	}
}
